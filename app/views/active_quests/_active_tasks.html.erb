<% if active_tasks != nil %>
    <%#> image of the map for the section and image for each task - like for tavern <%>
    <%#> box underneath the map that changes dependent on task selection <%>

    <% active_tasks.each do |task| %>

        <div id="task_partial" class="w-[600px]">
            <%= render partial: 'active_quests/task', locals: {task: task} %>
        </div>

    <% end %>

<% end %>